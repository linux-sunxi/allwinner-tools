;disp init configuration
;
;disp_mode            (0:screen0<screen0,fb0>)
;screenx_output_type  (0:none; 1:lcd; 3:hdmi;)
;screenx_output_mode  (used for hdmi output, 0:480i 1:576i 2:480p 3:576p 4:720p50)
;                     (5:720p60 6:1080i50 7:1080i60 8:1080p24 9:1080p50 10:1080p60)
;fbx format           (4:RGB655 5:RGB565 6:RGB556 7:ARGB1555 8:RGBA5551 9:RGB888 10:ARGB8888 12:ARGB4444)
;fbx pixel sequence   (0:ARGB 1:BGRA 2:ABGR 3:RGBA)
;fbx_width,fbx_height (framebuffer horizontal/vertical pixels, fix to output resolution while equal 0)
;lcdx_backlight       (lcd init backlight,the range:[0,256],default:197
;lcdx_yy              (lcd init screen bright/contrast/saturation/hue, value:0~100, default:50/50/57/50)
;----------------------------------------------------------------------------------
[disp_init]
disp_init_enable         = 1
disp_mode                = 0

screen0_output_type      = 1
screen0_output_mode      = 4

screen1_output_type      = 1
screen1_output_mode      = 4

fb0_format               = 10
fb0_pixel_sequence       = 0
fb0_scaler_mode_enable   = 0
fb0_width                = 0
fb0_height               = 0

fb1_format               = 10
fb1_pixel_sequence       = 0
fb1_scaler_mode_enable   = 0
fb1_width                = 0
fb1_height               = 0

lcd0_backlight           = 197
lcd1_backlight           = 197

lcd0_bright              = 50
lcd0_contrast            = 50
lcd0_saturation          = 57
lcd0_hue                 = 50

lcd1_bright              = 50
lcd1_contrast            = 50
lcd1_saturation          = 57
lcd1_hue                 = 50

;----------------------------------------------------------------------------------
;lcd0 configuration

;lcd_if:               0:hv(sync+de); 1:8080; 2:ttl; 3:lvds; 4:dsi
;lcd_x:                lcd horizontal resolution
;lcd_y:                lcd vertical resolution
;lcd_dclk_freq:        in MHZ unit
;lcd_dclk_phase:       lcd dclk phase(0:normal offset;1:1/3 offset; 2/3 offset;
;                      4:90 phase; 5:0 phase)
;lcd_pwm_freq:         in HZ unit
;lcd_pwm_pol:          lcd backlight PWM polarity
;lcd_hbp:              hsync back porch
;lcd_ht:               hsync total cycle
;lcd_vbp:              vsync back porch
;lcd_vt:               vysnc total cycle
;lcd_hspw:             hsync plus width
;lcd_vspw:             vysnc plus width
;lcd_hv_if:            0:parallel; 8:serial(8bit/3cycle); 10:dummyrgb(8bit/4cycle);
;                      11:rgbdummy(8bit/4cycle); 12: ccir656
;lcd_hv_srgb_seq:      serial RGB output sequence,
;lcd_hv_syuv_seq:      serial YUV output sequence
;lcd_hv_syuv_fdly:     serial YUV output F line delay(0: no delay;
;                      1: delay 2line[CCIR NTSC]; 2: delay 3line[CCIR PAL])
;lcd_lvds_if:          0:single link;  1:dual link
;lcd_lvds_colordepth:  0:8bit; 1:6bit
;lcd_lvds_mode:        0:NS mode; 1:JEIDA mode
;lcd_lvds_io_polarity: 0:normal; 1:pn cross
;lcd_dsi_if:           0:video mode; 1:command mode
;lcd_dsi_lane:		     1/2/3/4 lane
;lcd_dsi_format:       0:RGB888; 1:RGB666; 2:RGB666P; 3:RGB565
;lcd_dsi_eotp:         0:no ending symbol 1:insert ending symbol;
;lcd_dsi_te:           0:disable te mode; 1:rising te mode; 2:falling te mode
;lcd_cpu_if:           0:18bit; 1:16bit mode0; 2:16bit mode1; 3:16bit mode2;
;                      4:16bit mode3; 5:9bit; 6:8bit 256K; 7:8bit 65K;
;lcd_cpu_te:           0:disable te mode; 1:rising te mode; 2:falling te mode
;lcd_frm:              0:disable; 1:enable rgb666 dither; 2:enable rgb656 dither
;lcd_cpu_te:           0:disable trigger mode;    1: enable te trigger int mode
;lcd_gpio_0:           SCL
;lcd_gpio_1            SDA
;deu_mode              0:smoll lcd screen; 1:large lcd screen(larger than 10inch)
;lcdgamma4iep:         Smart Backlight parameter, lcd gamma vale * 10;
;                      decrease it while lcd is not bright enough; increase while lcd is too bright
;----------------------------------------------------------------------------------
[lcd0_para]
lcd_used            = 1

lcd_if              = 3
lcd_x               = 1280
lcd_y               = 800
lcd_dclk_freq       = 70
lcd_dclk_phase      = 1
lcd_pwm_freq        = 50000
lcd_pwm_pol         = 1
lcd_hbp             = 20
lcd_ht              = 1418
lcd_hspw            = 0
lcd_vbp             = 10
lcd_vt              = 814
lcd_vspw            = 0
lcd_hv_if           = 0
lcd_hv_srgb_seq     = 0
lcd_hv_syuv_seq     = 0
lcd_hv_syuv_fdly    = 0
lcd_lvds_if         = 0
lcd_lvds_colordepth = 1
lcd_lvds_mode       = 0
lcd_lvds_io_polarity= 0
lcd_dsi_if          = 0
lcd_dsi_lane        = 0
lcd_dsi_format      = 0
lcd_dsi_eotp        = 0
lcd_dsi_te          = 0
lcd_cpu_if          = 0
lcd_cpu_te          = 0
lcd_frm             = 1

deu_mode            = 0
